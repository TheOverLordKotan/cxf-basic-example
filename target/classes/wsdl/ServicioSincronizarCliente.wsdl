<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://org.itc/integracion/services/sifi/ServicioSincronizarCliente/V1"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:salesforceSifi="org:itc:salesforce:sifi:schema:xsd:CommonAggregateComponents-2"
	name="ServicioSincronizarCliente"
	targetNamespace="http://org.itc/integracion/services/sifi/ServicioSincronizarCliente/V1">

	<wsdl:types>
		<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema"
			xmlns:s="http://www.w3.org/2001/XMLSchema"
			targetNamespace="org:itc:salesforce:sifi:schema:xsd:CommonAggregateComponents-2"
			attributeFormDefault="qualified" elementFormDefault="qualified">
			<s:include
				schemaLocation="../schemas/ADA-CommonAggregateComponents-2.0.xsd" />
		</xsd:schema>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import
				namespace="org:itc:salesforce:sifi:schema:xsd:CommonAggregateComponents-2"
				schemaLocation="../schemas/ADA-CommonAggregateComponents-2.0.xsd">
			</xsd:import>
		</xsd:schema>
	</wsdl:types>

	<wsdl:message name="sincronizarCrmSifiRequest">
		<wsdl:part
			element="salesforceSifi:sincronizarCrmSifiRequest"
			name="sincronizarCrmSifiRequest" />
	</wsdl:message>
	<wsdl:message name="sincronizarCrmSifiResponse">
		<wsdl:part
			element="salesforceSifi:sincronizarCrmSifiResponse"
			name="sincronizarCrmSifiResponse" />
	</wsdl:message>



	<wsdl:message name="sincronizarCrmSifiFault">
		<wsdl:part name="sincronizarCrmSifiFault"
			element="salesforceSifi:sincronizarCrmSifiFault"></wsdl:part>
	</wsdl:message>

	<wsdl:message name="RequestHeader">
		<wsdl:part element="salesforceSifi:headerCrm"
			name="FiduHeader" />
	</wsdl:message>



	<wsdl:portType name="ServicioSincronizarCliente">
		<wsdl:operation name="sincronizarCrmSifi">
			<wsdl:input message="tns:sincronizarCrmSifiRequest" />
			<wsdl:output message="tns:sincronizarCrmSifiResponse" />
			<wsdl:fault name="fault"
				message="tns:sincronizarCrmSifiFault"></wsdl:fault>
		</wsdl:operation>
	</wsdl:portType>


	<wsdl:binding name="ServicioSincronizarClienteSOAP"
		type="tns:ServicioSincronizarCliente">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />

		<wsdl:operation name="sincronizarCrmSifi">
			<soap:operation
				soapAction="http://org.itc/integracion/services/sifi/ServicioSincronizarCliente/V1/adminClientePN" />
			<wsdl:input>
				<soap:header use="literal" part="FiduHeader"
					wsdl:required="true" message="tns:RequestHeader" />
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>

			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name="ServicioSincronizarCliente">
		<wsdl:port binding="tns:ServicioSincronizarClienteSOAP"
			name="ServicioSincronizarClienteSOAP">
			<soap:address
				location="http://org.itc/integracion/services/sifi/ServicioSincronizarCliente/V1" />
		</wsdl:port>
	</wsdl:service>


</wsdl:definitions>
