<!DOCTYPE xml>
<jaxb:bindings version="2.0"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<globalBindings>
		<serializable uid="1" />
	</globalBindings>
	<bindings schemaLocation="Autentication.wsdl"
		node="wsdl:definitions/types/schema[@targetNamespace='urn:fault.partner.soap.sforce.com']">
		<bindings node="//element[@name='describeGlobalTheme']">
			<factoryMethod name="DescribeGlobalTheme" />
		</bindings>
		<bindings node="//element[@name='describeLayout']">
			<factoryMethod name="DescribeApprovalLayout" />
		</bindings>
	</bindings>

</jaxb:bindings> 
